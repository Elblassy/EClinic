<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_main"
    app:startDestination="@id/navigation_contact">

    <fragment
        android:id="@+id/navigation_contact"
        android:name="app.iflatco.eclinic.ui.contact_dr.ContactDr"
        android:label="contact_dr_fragment"
        tools:layout="@layout/contact_dr_fragment" />
    <fragment
        android:id="@+id/navigation_profile"
        android:name="app.iflatco.eclinic.ui.profile.UserProfile"
        android:label="user_profile_fragment"
        tools:layout="@layout/user_profile_fragment" >
        <action
            android:id="@+id/action_navigation_profile_to_myAppointment"
            app:destination="@id/myAppointment" />
    </fragment>
    <fragment
        android:id="@+id/navigation_settings"
        android:name="app.iflatco.eclinic.ui.settings.Settings"
        android:label="settings_fragment"
        tools:layout="@layout/settings_fragment" />
    <fragment
        android:id="@+id/myAppointment"
        android:name="app.iflatco.eclinic.ui.appointment_patient.MyAppointment"
        android:label="my_appointment_fragment"
        tools:layout="@layout/my_appointment_fragment" />
</navigation>